#include <bits/stdc++.h>
using namespace std;

map<int, string> mp = {
    {100, "hundred"}, {90, "ninety"}, {80, "eighty"}, {70, "seventy"},
    {60, "sixty"}, {50, "fifty"}, {40, "forty"}, {30, "thirty"},
    {20, "twenty"}, {19, "nineteen"}, {18, "eighteen"}, {17, "seventeen"},
    {16, "sixteen"}, {15, "fifteen"}, {14, "fourteen"}, {13, "thirteen"},
    {12, "twelve"}, {11, "eleven"}, {10, "ten"}, {9, "nine"},
    {8, "eight"}, {7, "seven"}, {6, "six"}, {5, "five"},
    {4, "four"}, {3, "three"}, {2, "two"}, {1, "one"}, {0, "zero"}
};

string numberToWords(int nums)
{
    if (mp.count(nums)) {
        return mp[nums];
    }
    
    string result = mp[(nums / 10) * 10];
    int ones = nums % 10;

    if (ones > 0) {
        result += " " + mp[ones];
    }

    return result;
}

int countVowels(const string& s) {
    int count = 0;

    for (char c : s)
    {
        if (c == 'a' || c == 'e' || c == 'i' || c == 'o' || c == 'u') {
            count++;
        }
    }

    return count;
}

string stringPair(int n, vector<int>& num) {
    int D = 0;

    for (auto v : num)
    {
        string s = numberToWords(v);
        D += countVowels(s);
    }

    int pairCount = 0;
    unordered_map<int, int> freqMap;

    for (int i = 0; i < n; i++)
    {
        int complement = D - num[i];

        if (freqMap[complement] > 0) {
            pairCount += freqMap[complement];
        }

        freqMap[num[i]]++;
    }

    return numberToWords(pairCount);
}
