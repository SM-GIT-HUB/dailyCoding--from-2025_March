#include<bits/stdc++.h>
using namespace std;

#define enc '\n'

using int64 = int64_t;
using int128 = __int128;
using vint = vector<int>;
using vi64 = vector<int64_t>;
using pr2 = pair<int, int>;
using pr64 = pair<int64, int64>;

void myFunction()
{
	int n;
	cin >> n;
	
	vint arr(n);
	
	for (int i = 0; i < n; i++)
	{
		cin >> arr[i];
	}
	
	int streak = 0;
	int solved = 0;
	
	for (int x : arr)
	{
		if (x > 0) {
			++solved;
		}
		else
			solved = 0;
			
		streak = max(solved, streak);
	}
	
	cout << streak;
}

int main()
{
	ios::sync_with_stdio(false);
	cin.tie(nullptr);
	
	int t;
	cin >> t;
	
	while (t--)
	{
		myFunction();
		cout << enc;
	}
	
	return 0;
}
