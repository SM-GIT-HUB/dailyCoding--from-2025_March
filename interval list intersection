#include <bits/stdc++.h> 
vector<vector<int> > intersectionIntervals(vector<vector<int> >& interval1, vector<vector<int> >& interval2, int n1, int n2)
{
    // Write your code here.
    int i = 0;
    int j = 0;
    vector<vector<int> > ans;

    while (i < n1 && j < n2)
    {
        int L = max(interval1[i][0], interval2[j][0]);
        int R = min(interval1[i][1], interval2[j][1]);

        if (L <= R) {
            ans.push_back({L, R});
        }

        if (interval1[i][1] < interval2[j][1]) {
            i++;
        }
        else
            j++;
    }

    return ans;
}
