#include <bits/stdc++.h> 
vector<int> similarStrings(int n, string a, string b, string c) {
  // Write your code here.
  int mini = INT_MAX;
  int maxi = INT_MIN;

  for (int r = 0; r < n; r++)
  {
    int diff = 0;

    for (int i = 0; i < n; i++)
    {
      diff += abs(a[(i + r) % n] - b[i]) + abs(a[(i + r) % n] - c[i]);
    }

    mini = min(mini, diff);
    maxi = max(maxi, diff);
  }

  return {maxi, mini};
}
