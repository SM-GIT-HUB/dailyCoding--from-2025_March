#include <bits/stdc++.h>

int isRegular(string &str)
{
    int open = 0;

    for (char x : str)
    {
        if (x == '(') {
            open++;
        }
        else
        {
            if (open == 0) return 0;
            open--;
        }
    }

    return (open == 0);
}

string ninjaSubsequence(int n, string s, int k) {
    // Write your code here.
    if (!isRegular(s)) {
        return (k == 1)? s : "-1";
    }

    if (k > n) return "-1";

    for (int i = 0; i < n;)
    {
        if (s[i] == '(')
        {
            i++;
            continue;
        }

        int j = i;

        while (j < n && s[i] == s[j])
        {
            j++;
        }

        k--;

        if (k == 0)
        {
            string ans = s.substr(0, i) + s.substr(i + 1);
            return ans;
        }

        i = j;
    }

    for (int i = n - 1; i >= 0;)
    {
        if (s[i] == ')')
        {
            i--;
            continue;
        }

        int j = i;

        while (j >= 0 && s[i] == s[j])
        {
            j--;
        }

        k--;

        if (k == 0)
        {
            string ans = s.substr(0, i) + s.substr(i + 1);
            return ans;
        }

        i = j;
    }

    return "-1";
}
