#include <bits/stdc++.h> 

int comp(string &s1, string &s2)
{
    if (s1.size() > s2.size()) return 1;
    else if (s1.size() < s2.size()) return -1;

    for (int i = 0; i < s1.size(); i++)
    {
        if (s1[i] > s2[i]) return 1;
        else if (s1[i] < s2[i]) return -1;
    }

    return 0;
}

int getSeg(int i, string &s, string &seg)
{
    while (i < s.size() && s[i] != '.')
    {
        seg.push_back(s[i++]);
    }

    return (i + 1);
}

int compareVersions(string a, string b) 
{
    // Write your code here
    int m = a.size();
    int n = b.size();

    int i = 0, j = 0;

    while (i < m && j < n)
    {
        string seg1 = "";
        string seg2 = "";

        i = getSeg(i, a, seg1);
        j = getSeg(j, b, seg2);

        int c = comp(seg1, seg2);

        if (c != 0) return c;
    }

    if (m > n)
    {
        while (i < m)
        {
            if (a[i] != '.' && a[i] != '0') return 1;
            i++;
        }
    }
    else if (m < n)
    {
        while (j < n)
        {
            if (b[j] != '.' && b[j] != '0') return -1;
            j++;
        }
    }

    return 0;
}
