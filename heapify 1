#include<bits/stdc++.h>
using namespace std;

#define enc '\n'

using int64 = int64_t; using lint = long long; using int128 = __int128;
using vint = vector<int>; using vi64 = vector<int64>; using pr2 = pair<int, int>;
using pr64 = pair<int64, int64>; using vvint = vector<vint>; using vvi64 = vector<vi64>;

template<typename... Args>
void prnt(const Args&... args) { ((cout << args << ' '), ...); }

template<typename T>
void singleInput(int64 n, vector<T>& arr) { arr.resize(n); for (auto &x : arr) cin >> x; }

template<typename T>
void pairInput(int64 n, vector<T>& arr) { arr.resize(n); for (auto &x : arr) cin >> x.first >> x.second; }

bitset<200001> visited;

void myFunction()
{
	int n;
	vi64 a;
	
	cin >> n;
	singleInput(n, a);
	
	if (is_sorted(a.begin(), a.end()))
	{
		prnt("YES");
		return;
	}
	
	vi64 b = a;
	sort(b.begin(), b.end());
	
	visited.reset();
	
	for (int i = 0; i < n; i++)
	{
		if (visited[i]) continue;
		
		set<int> sett;
		
		for (int j = i + 1; j <= n; j = (j << 1))
		{
			visited[j - 1] = 1;
			sett.insert(a[j - 1]);
		}
		
		for (int j = i + 1; j <= n; j = (j << 1))
		{
			if (!sett.count(b[j - 1]))
			{
				prnt("NO");
				return;
			}
		}
	}
	
	prnt("YES");
}

int main()
{
	ios::sync_with_stdio(false);
	cin.tie(nullptr);
	
	int t;
	cin >> t;
	
	while (t--)
	{
		myFunction();
		cout << enc;
	}
	
	return 0;
}
