class Solution {
  public:
    bool stringStack(string &pat, string &tar) {
        // code here
        int j = tar.size() - 1;
        
        for (int i = pat.size() - 1; j >= 0 && i >= 0; i--)
        {
            if (pat[i] == tar[j]) j--;
            else i--;
        }
        
        return (j < 0);
    }
};
