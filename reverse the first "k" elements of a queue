#include <bits/stdc++.h> 
void reverse(queue<int>& q, int k)
{
    // Write your code here.
    vector<int> st;
    int n = q.size();

    for (int i = 0; i < k; i++)
    {
        st.push_back(q.front());
        q.pop();
    }
    
    while (st.size())
    {
        q.push(st.back());
        st.pop_back();
    }

    for (int i = 0; i < n - k; i++)
    {
        q.push(q.front());
        q.pop();
    }
}
