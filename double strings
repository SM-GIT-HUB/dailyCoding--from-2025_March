#include <bits/stdc++.h>
int findDoubleStrings(string &str) {
	// Write your code here.
	int n = str.size();

	unordered_set<string> sett;
	vector<vector<int> > dp(n, vector<int>(n));

	for (int j = n - 1; j >= 0; j--)
	{
		for (int i = j - 1; i >= 0; i--)
		{
			if (str[i] == str[j])
			{
				if (j == n - 1) {
					dp[i][j] = 1;
				}
				else
					dp[i][j] = dp[i + 1][j + 1] + 1;

				if (dp[i][j] >= j - i) {
					sett.insert(str.substr(i, 2 * (j - i)));
				}
			}
		}
	}

	return sett.size();
}
