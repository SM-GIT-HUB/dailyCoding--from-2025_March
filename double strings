#include <bits/stdc++.h>

int dp[202][202];

int findDoubleStrings(string &str) {
	// Write your code here.
	int n = int(str.size());
	memset(dp, 0, sizeof(dp));

	unordered_set<string> sett;

	for (int j = n - 1; j >= 0; j--)
	{
		for (int i = j - 1; i >= 0; i--)
		{
			if (str[i] == str[j]) {
				dp[i][j] = 1 + dp[i + 1][j + 1];
			}

			if (dp[i][j] >= j - i) {
				sett.insert(str.substr(i, (j - i) * 2));
			}
		}
	}

	return int(sett.size());
}
