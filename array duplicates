class Solution {
  public:
    vector<int> findDuplicates(vector<int>& arr) {
        // code here
        vector<int> ans;
        
        for (int i = 0; i < (int)arr.size(); i++)
        {
            int elem = abs(arr[i]);
            
            if (arr[elem - 1] < 0) {
                ans.push_back(elem);
            }
            
            arr[elem - 1] *= -1;
        }
        
        return ans;
    }
};
