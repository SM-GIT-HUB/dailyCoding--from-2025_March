class Solution {
  public:
    vector<int> cntInRange(vector<int>& arr, vector<vector<int> >& queries) {
        // code here
        sort(arr.begin(), arr.end());
        
        vector<int> ans;
        ans.reserve(queries.size());
        
        for (auto &q : queries)
        {
            int count = upper_bound(arr.begin(), arr.end(), q[1]) - lower_bound(arr.begin(), arr.end(), q[0]);
            ans.push_back(count);
        }
        
        return ans;
    }
};
