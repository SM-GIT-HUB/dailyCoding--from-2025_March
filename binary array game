#include<bits/stdc++.h>
using namespace std;

#define enc '\n'
#define long long long

using int64 = int64_t;
using int128 = __int128;
using vint = vector<int>;
using vi64 = vector<int64_t>;
using pr2 = pair<int, int>;
using pr64 = pair<int64, int64>;

template<typename... Args>
void prnt(const Args&... args)
{
    ((cout << args << ' '), ...);
}

void myFunction()
{
	int64 n;
	cin >> n;
	
	vint a(n);
	
	for (int i = 0; i < n; i++)
	{
		cin >> a[i];
	}
	
	if (a[0] || a.back()) {
		prnt("Alice");
	}
	else
		prnt("Bob");
}

int main()
{
	ios::sync_with_stdio(false);
	cin.tie(nullptr);
	
	int t;
	cin >> t;
	
	while (t--)
	{
		myFunction();
		cout << enc;
	}
	
	return 0;
}
