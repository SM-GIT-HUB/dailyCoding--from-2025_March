#include <bits/stdc++.h>

int prefix[102];
int dp[102][102];

int mm(int start, int end)
{
    if (start > end) {
        return 0;
    }

    if (start == end) {
        return (prefix[end + 1] - prefix[start]);
    }

    if (dp[start][end] != -1) {
        return dp[start][end];
    }

    int val = INT_MAX;

    for (int i = start; i <= end; i++)
    {
        int curr = mm(start, i - 1) + mm(i + 1, end);
        val = min(curr, val);
    }

    return dp[start][end] = (prefix[end + 1] - prefix[start]) + val;
}

int minCostBST(vector<int>& arr, vector<int>& freq){
    // Write your code here.
    int n = arr.size();

    memset(dp, -1, sizeof(dp));

    prefix[0] = 0;

    for (int i = 0; i < n; i++)
    {
        prefix[i + 1] = freq[i] + prefix[i];
    }

    return mm(0, n - 1);
}
