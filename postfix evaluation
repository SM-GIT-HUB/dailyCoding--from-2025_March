class Solution {
  public:
    string oprs = "+-*/^";
    
    int getCalc(int one, int two, char opr)
    {
        if (opr == '-') {
            return one - two;
        }
        else if (opr == '+') {
            return one + two;
        }
        else if (opr == '*') {
            return one * two;
        }
        else if (opr == '^') {
            return pow(one, two);
        }
        
        int curr = floor(one / (double)two);
        return curr;
    }
  
    int evaluatePostfix(vector<string>& arr) {
        // code here
        vector<int> stack;
        
        for (string &x : arr)
        {
            if (x.size() == 1 && oprs.find(x) != string::npos)
            {
                int two = stack.back();
                stack.pop_back();
                int one = stack.back();
                stack.pop_back();
                
                int curr = getCalc(one, two, x[0]);
                stack.push_back(curr);
            }
            else
                stack.push_back(stoi(x));
        }
        
        return stack.back();
    }
};
