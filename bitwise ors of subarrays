class Solution {
public:
    int subarrayBitwiseORs(vector<int>& arr) {
        unordered_set<int> sett;

        for (int i = 0; i < arr.size(); i++)
        {
            sett.insert(arr[i]);

            for (int j = i - 1; j >= 0; j--)
            {
                if ((arr[j] | arr[i]) == arr[j]) break;
                arr[j] |= arr[i];
                sett.insert(arr[j]);
            }
        }

        return sett.size();
    }
};
