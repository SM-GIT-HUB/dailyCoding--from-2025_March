**/

#include<bits/stdc++.h>
using namespace std;

#define enc '\n'

using int64 = int64_t;
using int128 = __int128;
using vint = vector<int>;
using vi64 = vector<int64_t>;
using pr2 = pair<int, int>;
using pr64 = pair<int64, int64>;

template<typename... Args>
void prnt(const Args&... args)
{
    ((cout << args << ' '), ...);
}

void myFunction()
{
	int64 n;
	cin >> n;
	
	if (n == (1 << __lg(n)))
	{
		cout << -1;
		return;
	}
	
	if (!(n & 1))
	{
		vi64 arr(n);
		
		arr[0] = n;
		arr[n - 1] = 1;
		
		for (int i = 2; i < n; i += 2)
		{
			arr[i - 1] = i + 1;
			arr[i - 1 + 1] = i;
		}
		
		swap(arr[0], arr[n - (1 << __lg(n)) - 1]);
		
		for (auto x : arr)
		{
			prnt(x);
		}
	}
	else
	{
		prnt(n - 1);
		
		for (int i = 2; i < n - 2; i += 2)
		{
			prnt(i + 1);
			prnt(i);
		}
		
		prnt(n, 1);
	}
}

int main()
{
	ios::sync_with_stdio(false);
	cin.tie(nullptr);
	
	int t;
	cin >> t;
	
	while (t--)
	{
		myFunction();
		cout << enc;
	}
	
	return 0;
}
