bool isGoodNumber(int n) {
    // Write your code here.
    unordered_map<int, bool> mapp;

    while (!mapp[n] && n != 1)
    {
        mapp[n] = true;
        int newNum = 0;

        while (n != 0)
        {
            newNum += (n % 10) * (n % 10);
            n = n / 10;
        }

        n = newNum;
    }

    return (n == 1);
}
