#include <bits/stdc++.h>

bitset<301> rows;
bitset<301> cols;

vector<vector<int> > findGoodMatrix(vector<vector<int> >& arr)
{
    // Write your code here
    int m = (int)arr.size();
    int n = (int)arr[0].size();

    rows.reset();
    cols.reset();

    for (int i = 0; i < m; i++)
    {
        for (int j = 0; j < n; j++)
        {
            if (!arr[i][j]) {
                rows[i] = cols[j] = 1;
            }
        }
    }

    for (int i = 0; i < m; i++)
    {
        for (int j = 0; j < n; j++)
        {
            if (rows[i] || cols[j]) {
                arr[i][j] = 0;
            }
        }
    }

    return arr;
}
