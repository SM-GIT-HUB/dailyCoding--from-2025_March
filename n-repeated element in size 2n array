class Solution {
public:
    int repeatedNTimes(vector<int>& nums) {
        int n = nums.size();
        n = n >> 1;
        int mapp[10001] = {0};

        for (int x : nums)
        {
            if (++mapp[x] == n) {
                return x;
            }
        }
        
        return -1;
    }
};
