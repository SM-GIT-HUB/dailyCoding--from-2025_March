#include <bits/stdc++.h> 
vector<int> linearProbing(vector<int> keys)
{
    // Write your code here.
    int n = keys.size();
    vector<int> ans(n, -1);

    for (int x : keys)
    {
        int idx = x % n;

        while (ans[idx] != -1)
        {
            idx = (idx + 1) % n;
        }

        ans[idx] = x;
    }

    return ans;
}
