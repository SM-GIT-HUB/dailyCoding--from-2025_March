#include <bits/stdc++.h>

class DSU {
public:
    int parent[501];

    DSU(int n)
    {
        for (int i = 0; i < n; i++)
        {
            parent[i] = i;
        }
    }

    int findParent(int u)
    {
        return (parent[u] == u)? u : parent[u] = findParent(parent[u]);
    }

    void unionBy(int u, int v)
    {
        parent[findParent(u)] = parent[findParent(v)];
    }
};

vector<int> linearProbing(vector<int> keys)
{
    // Write your code here.
    int n = keys.size();
    DSU ds(n);

    vector<int> ans(n);
    
    for (int x : keys)
    {
        int idx = ds.findParent(x % n);
        ans[idx] = x;

        ds.unionBy(idx, (idx + 1) % n);
    }

    return ans;
}
