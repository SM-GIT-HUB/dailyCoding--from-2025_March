TrieNode* solution::deleteWord(TrieNode* root, string word) {
  // Write your code here
  TrieNode* curr = root;

  for (char x : word)
  {
    if (!curr->children[x - 'a']) return root;
    curr = curr->children[x - 'a'];
  }

  curr->isEnd = false;
  return root;
}

