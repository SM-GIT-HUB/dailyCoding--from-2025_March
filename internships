#include <bits/stdc++.h> 

int dfs(int person, int m, int n, vector<int>& match, vector<vector<int> >& g, bitset<101>& visited)
{
    visited[person] = 1;

    for (int i = 0; i < n; i++)
    {
        if (!g[person][i]) continue;

        if (match[i] == -1)
        {
            match[i] = person;
            return 1;
        }
        else
        {
            if (!visited[match[i]] && dfs(match[i], m, n, match, g, visited))
            {
                match[i] = person;
                return 1;
            }
        }
    }

    return 0;
}

int internships(int m, int n, vector<vector<int> >& g){
    // Write your code here.
    bitset<101> visited;
    vector<int> match(n, -1);

    int ans = 0;

    for (int person = 0; person < m; person++)
    {
        visited.reset();

        if (dfs(person, m, n, match, g, visited)) {
            ans++;
        }
    }

    return ans;
}
