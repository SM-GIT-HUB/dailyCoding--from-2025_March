#include <bits/stdc++.h> 
vector<string> printPattern(int n)
{
	// Write your code here
	int curr = 1;
	vector<string> ans(n);
	
	int idx = 0;
	int start = 0, end = n - 1;

	while (start <= end)
	{
		for (int i = 0; i < n; i++)
		{
			ans[idx].push_back(to_string(curr));
			curr++;
		}

		if (idx == start)
		{
			start++;
			idx = end;
		}
		else
		{
			end--;
			idx = start;
		}
	}

	return ans;
}
