#include <bits/stdc++.h>

int mapp[500];

vector<int> nextGreaterElement(int n, vector<int> A, int m, vector<int> B) {
    // Write your code here.
	vector<int> next(m);
	vector<int> stack = {-1};

	for (int i = m - 1; i >= 0; i--)
	{
		while (stack.back() != -1 && stack.back() <= B[i])
		{
			stack.pop_back();
		}

		next[i] = stack.back();
		stack.push_back(B[i]);
		mapp[B[i]] = i;
	}

	vector<int> ans;
	for (int x : A) ans.push_back(next[mapp[x]]);

	return ans;
}
