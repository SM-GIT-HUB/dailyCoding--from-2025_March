#include <bits/stdc++.h> 
vector< int > corpFlightBookings(vector< vector < int > > &bookings, int n) {
	// Write your code here.
	vector<int> arr(n + 1, 0);

	for (auto &v : bookings)
	{
		arr[v[0] - 1] += v[2];
		arr[v[1]] -= v[2];
	}

	int sum = 0;
	vector<int> ans;

	for (int i = 0; i < n; i++)
	{
		sum += arr[i];
		ans.push_back(sum);
	}

	return ans;
}
