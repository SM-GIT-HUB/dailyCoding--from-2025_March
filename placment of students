#include <bits/stdc++.h>

int assign[3001];
bitset<3001> visited;

void reset(int n)
{
    for (int i = 0; i < n; i++)
    {
        visited[i] = 0;
    }
}

int track(int s, int n, vector<vector<int> >& mat)
{
    for (int j = 0; j < n; j++)
    {
        if (!visited[j] && mat[s][j])
        {
            visited[j] = 1;
            
            if (assign[j] < 0 || track(assign[j], n, mat))
            {
                assign[j] = s;
                return 1;
            }
        }
    }

    return 0;
}

int maxMatch(vector<vector<int> >& mat) 
{
    // Write your code here
    int m = mat.size();
    int n = mat[0].size();

    for (int i = 0; i < n; i++)
    {
        assign[i] = -1;
    }

    int count = 0;

    for (int s = 0; s < m; s++)
    {
        reset(n);

        if (track(s, n, mat)) {
            count++;
        }
    }

    return count;
}
