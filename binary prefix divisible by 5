class Solution {
public:
    vector<bool> prefixesDivBy5(vector<int>& nums) {
        int val = 0;
        vector<bool> ans(nums.size());

        for (int i = 0; i < nums.size(); i++)
        {
            val = (val * 2 + nums[i]) % 5;
            ans[i] = (val == 0);
        }

        return ans;
    }
};
