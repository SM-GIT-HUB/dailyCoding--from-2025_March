class Solution {
  public:
    void rearrangeQueue(queue<int>& q) {
        // code here
        int n = (int)q.size();
        
        queue<int> q1;
        
        for (int i = 0; i < n / 2; i++)
        {
            q1.push(q.front());
            q.pop();
        }
        
        for (int i = 0; i < n / 2; i++)
        {
            q.push(q1.front());
            q1.pop();
            
            q.push(q.front());
            q.pop();
        }
    }
};
