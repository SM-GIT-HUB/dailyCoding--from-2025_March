class Solution {
public:
    int mapp[101] = {0};

    int maxFrequencyElements(vector<int>& nums) {
        int maxi = 0;
        int count = 0;

        for (int x : nums)
        {
            int c = ++mapp[x];
            
            if (c == maxi) {
                count++;
            }
            else if (c > maxi)
            {
                maxi = c;
                count = 1;
            }
        }

        return count * maxi;
    }
};
