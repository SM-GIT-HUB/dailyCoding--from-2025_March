#include <bits/stdc++.h> 
int getNewNum(vector<int> arr, int n) 
{
    // Write your code here.
	int mapp[31] = {0};

	for (int bit = 30; bit >= 0; bit--)
	{
		for (int x : arr) mapp[bit] += ((x >> bit) & 1);
	}

	int ans = 0;

	for (int bit = 30; bit >= 0; bit--)
	{
		if (mapp[bit] > n - mapp[bit]) ans = ans | (1 << bit);
	}

	return ans;
}
