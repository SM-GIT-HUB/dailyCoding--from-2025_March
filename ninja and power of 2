#include <bits/stdc++.h>

vector<vector<int> > numberPattern(int n) {
    // Write your code here
    int elem = 0;
    vector<vector<int> > arr(n, vector<int>(1 << n, -1));

    for (int i = 0; i < n; i++)
    {
        for (int c = 0; c < (1 << i); c++)
        {
            arr[i][c] = elem + 1;
            elem = (elem + 1) % 9;
        }
    }

    return arr;
}
