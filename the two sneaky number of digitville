class Solution {
public:
    vector<int> getSneakyNumbers(vector<int>& nums) {
        vector<int> ans;
        ans.reserve(2);
        
        bitset<101> visited;

        for (int x : nums)
        {
            if (visited[x]) ans.push_back(x);
            else visited[x] = 1;
        }

        return ans;
    }
};
