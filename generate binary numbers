class Solution {
  public:
    vector<string> generateBinary(int n) {
        // code here
        vector<string> ans;
        ans.push_back("1");
        
        int idx = 0;
        
        while (ans.size() < n)
        {
            string curr = ans[idx];
            
            ans.emplace_back(curr + "0");
            if (ans.size() == n) break;
            
            ans.emplace_back(curr + "1");
            idx++;
        }
        
        return ans;
    }
};
