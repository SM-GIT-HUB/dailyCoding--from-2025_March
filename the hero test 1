#include <bits/stdc++.h>

bitset<3002> visited;

vector<int> theOrder(int n)
{
	// Write your code here.
	visited.reset();
	vector<int> ans;

	int idx = 1;

	while (ans.size() != n)
	{
		if (visited[idx])
		{
			idx = (idx + 1) % n;
			continue;
		}

		visited[idx] = 1;
		ans.push_back(idx + 1);

		if (ans.size() == n) break;

		int i = (idx + 1) % n;

		while (visited[i])
		{
			i = (i + 1) % n;
		}

		idx = (i + 1) % n;
	}

	return ans;
}
