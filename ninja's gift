#include <bits/stdc++.h> 
bool canChange(string &k, string &s){
    // write your code here.
    int n = s.size();
    if (k.size() != n) return 0;

    int mapp[26];
    for (int i = 0; i < 26; i++) mapp[i] = -1;

    for (int i = 0; i < n; i++)
    {
        char x = k[i];
        char y = s[i];

        if (mapp[x - 'a'] != -1)
        {
            if (mapp[x - 'a'] == (y - 'a')) continue;
            else return 0;
        }

        mapp[x - 'a'] = (y - 'a');
    }

    return 1;
}
