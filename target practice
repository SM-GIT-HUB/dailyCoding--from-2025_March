#include<bits/stdc++.h>
using namespace std;

#define enc '\n'

using int64 = int64_t; using lint = long long; using int128 = __int128;
using vint = vector<int>; using vi64 = vector<int64>; using pr2 = pair<int, int>;
using pr64 = pair<int64, int64>; using vvint = vector<vint>; using vvi64 = vector<vi64>;

template<typename... Args>
void prnt(const Args&... args) { ((cout << args << ' '), ...); }

template<typename T>
void singleInput(int64 n, vector<T>& arr) { arr.resize(n); for (auto &x : arr) cin >> x; }

template<typename T>
void pairInput(int64 n, vector<T>& arr) { arr.resize(n); for (auto &x : arr) cin >> x.first >> x.second; }

void myFunction()
{
	string s[10];
	for (auto &str : s) cin >> str;
	
	int up = 0, left = 0, right = 9, down = 9;
	
	int sc = 1;
	int points = 0;
	
	while (up < down)
	{
		int c = 0;
		
		for (int i = left; i <= right; i++) c += (s[up][i] == 'X');
		up++;
		for (int i = up; i <= down; i++) c += (s[i][right] == 'X');
		right--;
		for (int i = right; i >= left; i--) c += (s[down][i] == 'X');
		down--;
		for (int i = down; i >= up; i--) c += (s[i][left] == 'X');
		++left;
		
		points += c * sc;
		++sc;
	}
	
	prnt(points);
}

int main()
{
	ios::sync_with_stdio(false);
	cin.tie(nullptr);
	
	int t;
	cin >> t;
	
	while (t--)
	{
		myFunction();
		cout << enc;
	}
	
	return 0;
}
