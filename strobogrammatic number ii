#include <bits/stdc++.h>

string singleMatch[3] = {"1", "8", "0"};
string match[5] = {"96", "69", "11", "88", "00"};

void track(int start, int end, string &temp, vector<string>& ans)
{
	if (start > end)
	{
		ans.push_back(temp);
		return;
	}

	if (start == end)
	{
		for (int i = 0; i < 3; i++)
		{
			temp[start] = singleMatch[i][0];
			track(start + 1, end - 1, temp, ans);
		}
		
		return;
	}

	for (int i = 0; i < 5; i++)
	{
		if (start == 0 && match[i][0] == '0') {
			continue;
		}

		temp[start] = match[i][0];
		temp[end] = match[i][1];

		track(start + 1, end - 1, temp, ans);
	}
}

vector<string> findStrobogrammatic(int n) {
	// Write your code here.
	if (n == 1) {
		return {"0", "1", "8"};
	}

	vector<string> ans;
	string temp = string(n, '0');

	track(0, n - 1, temp, ans);

	return ans;
}
