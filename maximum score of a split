class Solution {
public:
    long long maximumScore(vector<int>& nums) {
        long long prefix = 0;
        int minVal = nums.back();

        for (int x : nums)
        {
            prefix += x;
        }

        prefix -= minVal;
        long long ans = INT_MIN;lee

        for (int i = nums.size() - 2; i >= 0; i--)
        {
            ans = max(ans, prefix - minVal);
            prefix -= nums[i];
            minVal = min(nums[i], minVal);
        }

        return ans;
    }
};
