#include <bits/stdc++.h>

vector<string> arr;

void wordFilter(vector<string> &words)
{
    //  Write your code here.
    arr = words;
}

int find(string &prefix, string &suffix)
{
    //  Write your code here.
    int ans = -1;

    for (int idx = 0; idx < arr.size(); idx++)
    {
        string &str = arr[idx];

        int n = str.size();
        int pn = prefix.size();
        int sn = suffix.size();

        int i = 0;
        int j = n - 1;
        int si = sn - 1;

        while (i < pn || si >= 0)
        {
            if (i < pn && (i >= n || str[i] != prefix[i])) break;
            if (si >= 0 && (j < 0 || str[j] != suffix[si])) break;

            i++;
            j--;
            si--;
        }

        if (i >= pn && si < 0)
        {
            if (ans == -1 || (str.size() >= arr[ans].size())) ans = idx;
        }
    }

    return ans;
}
