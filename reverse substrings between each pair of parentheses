#include <bits/stdc++.h> 
string reverseStringsInParentheses(string s, int n){

    // Write your code here.
    // This function returns the reversed string output without the parentheses.
    vector<char> stack;

    for (char x : s)
    {
        if (x != ')') {
            stack.push_back(x);
        }
        else
        {
            string temp = "";

            while (!stack.empty() && stack.back() != '(')
            {
                temp.push_back(stack.back());
                stack.pop_back();
            }

            stack.pop_back();
            for (char x : temp) stack.push_back(x);
        }
    }

    string ans = "";
    for (char x : stack) ans.push_back(x);

    return ans;
}
