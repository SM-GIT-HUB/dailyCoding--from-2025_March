#include <bits/stdc++.h> 
vector<int> findLeaders(vector<int>& arr, int n) {
    // Write your code here.
    vector<int> ans;
    int maxi = INT_MIN;

    for (int i = n - 1; i >= 0; i--)
    {
        if (arr[i] > maxi)
        {
            maxi = arr[i];
            ans.push_back(maxi);
        }
    }

    reverse(ans.begin(), ans.end());

    return ans;
}
