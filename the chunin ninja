#include <bits/stdc++.h> 
vector <int> chuninNinja(int m, int n, vector<vector<int> > arr){
   // Write your code here.
   vector<int> rows(m, 1e9);
   vector<int> cols(n);

   for (int i = 0; i < m; i++)
   {
      for (int j = 0; j < n; j++)
      {
         rows[i] = min(rows[i], arr[i][j]);
         cols[j] = max(arr[i][j], cols[j]);
      }
   }

   vector<int> ans;

   for (int i = 0; i < m; i++)
   {
      for (int j = 0; j < n; j++)
      {
         if (arr[i][j] == rows[i] && arr[i][j] == cols[j]) ans.push_back(arr[i][j]);
      }
   }

   return ans;
}
