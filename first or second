#include<bits/stdc++.h>
using namespace std;

#define enc '\n'

using int64 = int64_t; using lint = long long; using int128 = __int128;
using vint = vector<int>; using vi64 = vector<int64>; using pr2 = pair<int, int>;
using pr64 = pair<int64, int64>; using vvint = vector<vint>; using vvi64 = vector<vi64>;

template<typename... Args>
void prnt(const Args&... args) { ((cout << args << ' '), ...); }

template<typename T>
void singleInput(int64 n, vector<T>& arr) { arr.resize(n); for (auto &x : arr) cin >> x; }

template<typename T>
void pairInput(int64 n, vector<T>& arr) { arr.resize(n); for (auto &x : arr) cin >> x.first >> x.second; }

void myFunction()
{
	int64 n;
	cin >> n;
	
	vi64 a;
	singleInput(n, a);
	
	vi64 prefix(n);
	vi64 suffix(n);
	
	for (int i = 1; i < n; i++)
	{
		prefix[i] = prefix[i - 1] + abs(a[i]);
	}
	
	for (int i = n - 2; i >= 0; i--)
	{
		suffix[i] = suffix[i + 1] - a[i + 1];
	}
	
	int64 ans = suffix[0];
	
	for (int i = 1; i < n; i++)
	{
		ans = max(ans, a[0] + prefix[i - 1] + suffix[i]);
	}
	
	prnt(ans);
}

int main()
{
	ios::sync_with_stdio(false);
	cin.tie(nullptr);
	
	int t;
	cin >> t;
	
	while (t--)
	{
		myFunction();
		cout << enc;
	}
	
	return 0;
}
