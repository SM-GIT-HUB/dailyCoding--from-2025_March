#include <bits/stdc++.h>

int mapp[1002];

int findMaxElement(vector<pair<int,int> >& arr, int n, int m) {
    // Write Your Code here.
    memset(mapp, 0, sizeof(mapp));

    for (auto [L, R] : arr)
    {
        ++mapp[L];
        --mapp[R + 1];
    }

    int maxi = 0;

    for(int i = 1; i <= n; i++)
    {
        mapp[i] += mapp[i - 1];
        maxi = max(mapp[i], maxi);
    }

    return maxi;
}
