class Solution {
public:
    vector<int> intersection(vector<vector<int> >& nums) {
        vector<int> ans;
        int mapp[1001] = {0};

        for (auto &arr : nums)
        {
            for (int x : arr)
            {
                ++mapp[x];
            }
        }

        int n = nums.size();

        for (int i = 1; i < 1001; i++)
        {
            if (mapp[i] == n) {
                ans.push_back(i);
            }
        }

        return ans;
    }
};
