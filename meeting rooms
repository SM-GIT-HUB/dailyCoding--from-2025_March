class Solution {
  public:
    bool canAttend(vector<vector<int> >& arr) {
        // Code Here
        sort(arr.begin(), arr.end());
        
        for (int i = 0; i < (int)arr.size() - 1; i++)
        {
            if (arr[i][1] > arr[i + 1][0]) {
                return 0;
            }
        }
        
        return 1;
    }
};
