class Solution {
  public:
    int maxSubarraySum(vector<int>& arr, int k) {
        // code here
        int sum = 0;
        int n = arr.size();

        for (int i = 0; i < k; i++)
        {
            sum += arr[i];
        }
        
        int maxi = sum;
        
        for (int i = 1; i + k - 1 < n; i++)
        {
            sum += arr[i + k - 1] - arr[i - 1];
            maxi = max(sum, maxi);
        }
        
        return maxi;
    }
};
