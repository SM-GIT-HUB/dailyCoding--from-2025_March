#include <bits/stdc++.h> 
int countAP(int n, vector<int> arr)
{
    // Write your code here.
    int ans = 0;
    vector<unordered_map<long long, int> > dp(n);

    for (int i = 0; i < n; i++)
    {
        for (int j = 0; j < i; j++)
        {
            long long diff = (long long)arr[i] - arr[j];
            int cnt = (dp[j].count(diff))? dp[j][diff] : 0;

            ans += cnt;
            dp[i][diff] += cnt + 1;
        }
    }

    return ans;
}
